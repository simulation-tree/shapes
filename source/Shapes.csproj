<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <ImplicitUsings>disable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
        <Title>Shapes</Title>
        <Authors>popcron</Authors>
        <Company>simulation-tree</Company>
        <RepositoryUrl>https://github.com/simulation-tree/shapes</RepositoryUrl>
        <IsAotCompatible>True</IsAotCompatible>
        <IsTrimmable>True</IsTrimmable>
        <TreatWarningsAsErrors>True</TreatWarningsAsErrors>
        <PackageId/>
    </PropertyGroup>

    <ItemGroup>
        <ProjectReference Include="..\..\unmanaged\core\Unmanaged.Core.csproj"/>
        <ProjectReference Include="..\..\types\core\Types.Core.csproj"/>
        <ProjectReference Include="..\..\types\generator\Types.Generator.csproj">
            <OutputItemType>Analyzer</OutputItemType>
            <ReferenceOutputAssembly>false</ReferenceOutputAssembly>
        </ProjectReference>
    </ItemGroup>

</Project>